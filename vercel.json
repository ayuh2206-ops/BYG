{
  "cleanUrls": true,
  "trailingSlash": false,
  "rewrites": [
    {
      "source": "/traveler/:path*",
      "destination": "/pages/traveler/:path*"
    },
    {
      "source": "/guide/:path*",
      "destination": "/pages/guide/:path*"
    },
    {
      "source": "/agency/:path*",
      "destination": "/pages/agency/:path*"
    },
    {
      "source": "/settings/:path*",
      "destination": "/pages/shared/:path*"
    }
  ],
  "redirects": [
    {
      "source": "/dashboard",
      "destination": "/pages/traveler/dashboard.html",
      "statusCode": 302
    },
    {
      "source": "/signin",
      "destination": "/pages/public/signin.html",
      "statusCode": 302
    },
    {
      "source": "/signup",
      "destination": "/pages/public/signin.html",
      "statusCode": 302
    },
    {
      "source": "/quiz",
      "destination": "/pages/public/quiz.html",
      "statusCode": 302
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        }
      ]
    }
  ]
}